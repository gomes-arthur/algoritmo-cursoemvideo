algoritmo "MeuNome"
// Função :
// Autor :
// Data : 24/03/2022
// Seção de Declarações
var
   n1, n2, s: inteiro
inicio
      escreva ("Digite um número: ")
      leia (n1)
      escreva ("Digite outro número para somar com o anterior: ")
      leia (n2)
      s <- n1 + n2
      escreva ("A soma entre", n1, " e", n2, " é igual a:",s)
fimalgoritmo
