Algoritmo "Exercicio15"

Var
   Dias, Salario: Inteiro
Inicio
      EscrevaL("---------------------------------")
      EscrevaL("SALÁRIO DO MêS POR DIA TRABALHADO")
      EscrevaL("---------------------------------")
      Escreva("Quantos dias você trabalhou no mês? ")
      Leia(Dias)
      Salario <- Dias * 25 * 8
      Escreva("O seu salário é de R$", Salario, ".")
Fimalgoritmo