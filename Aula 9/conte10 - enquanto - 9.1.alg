algoritmo "conteAte10"
// Função : contar de 0 até 10.
// Autor :
// Data : 04/04/2022
// Seção de Declarações 
var
   contador: inteiro
inicio
      contador <- 0
      enquanto (contador <= 10) faca
               escreval (contador)
               contador <- contador + 1
      fimEnquanto
			escreval ("Terminei de contar.")
fimalgoritmo
